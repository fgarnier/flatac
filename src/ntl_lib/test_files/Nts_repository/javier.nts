nts javier;

main {

  x,y : int;

  initial s0;
  final s9;
  error s8;

  s0 -> s1 { x' = 0 }
  s1 -> s2 { y' = 0 && x' = x }
  s2 -> s3 { x > 0 && y' = y }
  s2 -> s5 { !(x > 0) && y' = y }
  s3 -> s4 { x' = -x && y' = y }
  s5 -> s6 { y > 0 && x' = x }
  s4 -> s7 { y' = -y && x' = x }
  s6 -> s7 { y' = -y && x' = x }
  s7 -> s8 { !(x + y <= 0) && x' = x && y' = y }
  s7 -> s9 { (x + y <= 0) && x' = x && y' = y }
}
