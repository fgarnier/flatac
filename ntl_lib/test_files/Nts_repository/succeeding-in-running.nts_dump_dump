nts succrunning ; 
main{

x,b,y : int;
initial q0;

error qe; 
 q0->q1 { x' = 1000 and y' = 0 and havoc(b,x,y) } 
 q1->q2 { x > 0 and x' = x-1 and havoc(x) } 
 q1->q4 { x <=  0 and havoc() } 
 q2->q1 { y' = y+2 and havoc(y) } 
 q2->q2 { b != 0 and y' = 2*(y+x) and havoc(b,y) } 
 q4->qe { not (y != 47 and x = 0) and havoc() }
}

 