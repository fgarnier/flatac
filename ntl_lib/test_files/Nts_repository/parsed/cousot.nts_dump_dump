nts cousot ; 
main{

tmp,y,x : int;
initial sinit;

error se; 
 s1->s2 { havoc(tmp) } 
 s2->s3 { tmp != 0 and havoc() } 
 s2->s5 { tmp = 0 and havoc() } 
 s3->s4 { x' = x+1 and havoc(x) } 
 s4->s1 { y' = y+1 and havoc(y) } 
 s5->s6 { havoc() } 
 s6->s7 { x > 0 and havoc() } 
 s6->s8 { x <=  0 and havoc() } 
 s7->s9 { x' = x-1 and havoc(x) } 
 s8->se { y != 0 and havoc() } 
 s9->s5 { y' = y-1 and havoc(y) } 
 sinit->s1 { x' = 0 and y' = 0 and havoc(x,y) }
}

 