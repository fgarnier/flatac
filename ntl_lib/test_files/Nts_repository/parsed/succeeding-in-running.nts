nts succrunning;

main {
  x,b,y : int;
  initial q0;
  error qe;
  q0 ->  q1 { x' = 1000 && y' = 0 && havoc(b,x,y) }
  q1 ->  q2 { x > 0 && x' = x - 1 && havoc(x) }
  q2 ->  q2 { b != 0 && y' = 2 * (y + x) &&  havoc(b,y) }
  q2 ->  q1 { y' = y + 2 && havoc(y) }
  q1 ->  q4 { x <= 0 && havoc() }
  q4 ->  qe { !(y != 47 && x = 0) && havoc() }
}
